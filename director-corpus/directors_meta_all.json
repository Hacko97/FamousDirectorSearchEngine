bc7\u0bb2\u0bcd \u0baa\u0bb5\u0bb2\u0bcd", "\u0b9f\u0bbe\u0bae\u0bcd \u0b9f\u0bbf\u0b95\u0bcd\u0bb5\u0bc6\u0bb0\u0bcd", "\u0b9a\u0bbe\u0bae\u0bcd \u0baa\u0bc7\u0b95\u0bcd\u0b95\u0bbf\u0b99\u0bcd\u0baa\u0bb9\u0bcd", "\u0bb2\u0bc2\u0baf\u0bbf\u0bb8\u0bcd \u0bae\u0bb2\u0bcd\u0bb2\u0bc7", "\u0b9c\u0bca\u0ba9\u0bbe\u0ba4\u0ba9\u0bcd \u0b9f\u0bc0\u0bae\u0bcd\u0bae\u0bc7", "\u0b9a\u0bc6\u0bb0\u0bcd\u0b9c\u0bbf\u0baf\u0bcb \u0bb2\u0bc7\u0b9a\u0bbe\u0ba9", "\u0b9a\u0bae\u0bcd\u0bb8\u0bcd \u0bb2\u0bcd. \u0baa\u0bcd\u0bb0\u0bc2\u0b95\u0bcd\u0bb8\u0bcd", "\u0bae\u0bc8\u0b95\u0bc7\u0bb2\u0bcd \u0b9a\u0bc1\u0bb0\u0bcd\u0b9f\u0bbf\u0bb8\u0bcd", "\u0b9c\u0bbe\u0ba9\u0bcd \u0b9a\u0bbe\u0baf\u0bb2\u0bc6\u0bb8\u0bcd", "\u0baa\u0bc6\u0b9f\u0bcd\u0bb0\u0bbe \u0b85\u0bb2\u0bc8\u0bae\u0bcb\u0ba4\u00f3\u0bb5\u0bb0", "\u0b9f\u0bc7\u0ba9\u0bbf \u0baa\u0bbe\u0baf\u0bcd\u0bb2\u0bc7", "\u0bb5\u0bbe\u0bb0\u0ba9\u0bcd \u0baa\u0bc0\u0b9f\u0bcd\u0b9f\u0bbf", "\u0b86\u0bb2\u0ba9\u0bcd \u0b9c\u0bc7. \u0baa\u0bcb\u0b95\u0bcd\u0b95\u0bc1\u0bb2", "\u0b85\u0bb2\u0bcd\u0baa\u0bcb\u0ba9\u0bcd\u0b9a\u0bcb \u0b9a\u0bc1\u0bae\u0bbe\u0bb0\u0bcd\u00f3\u0ba9\u0bcd", "\u0b86\u0bb2\u0ba9\u0bcd \u0baa\u0bbe\u0bb0\u0bcd\u0b95\u0bcd\u0b95\u0bb0\u0bcd", "\u0b9f\u0bbf\u0bae\u0bcd \u0baa\u0bb0\u0bcd\u0b9f\u0ba9\u0bcd", "\u0b85\u0ba8\u0bcd\u0ba4\u0bcb\u0ba3\u0bbf \u0bae\u0bbf\u0b99\u0bcd\u0b95\u0bcd\u0b95\u0bc7\u0bb2\u0bcd\u0bb2", "\u0b9c\u0bbf\u0bae\u0bcd \u0bb7\u0bc6\u0bb0\u0bbf\u0b9f\u0ba9\u0bcd", "\u0bb2\u0bbe\u0bb0\u0ba9\u0bcd\u0bb8\u0bcd \u0b92\u0bb3\u0bbf\u0bb5\u0bc0\u0bb0\u0bcd", "\u0b9c\u0bbe\u0bb0\u0bcd\u0b9c\u0bcd \u0b9a\u0bc1\u0b95\u0bcd\u0b95\u0bcb\u0bb0\u0bcd", "\u0bb0\u0bbf\u0b9f\u0bcd\u0bb2\u0bbf \u0bb8\u0bcd\u0b95\u0bbe\u0b9f\u0bcd", "\u0baa\u0bb8\u0bcd \u0bb2\u0bc1\u0bb9\u0bcd\u0bb0\u0bcd\u0bae\u0ba9\u0bbf\u0ba9\u0bcd", "\u0b9c\u0bc0\u0ba9\u0bcd \u0bb0\u0bc6\u0ba9\u0bcb\u0baf\u0bcd\u0bb1\u0bcd", "\u0ba8\u0bc0\u0bb2\u0bcd \u0b9c\u0bcb\u0bb0\u0bcd\u0b9f\u0bbe\u0ba9\u0bcd", "\u0b8e\u0bb0\u0bbf\u0b95\u0bcd \u0bb5\u0bcb\u0ba9\u0bcd \u0bb8\u0bcd\u0b9f\u0bcd\u0bb0\u0bcb\u0bb9\u0bc6\u0baf\u0bcd\u0bae\u0bcd", "\u0baa\u0bbe\u0baa\u0bcd \u0baa\u0bcb\u0bb8\u0bc7", "\u0b9f\u0bc6\u0bb0\u0bcd\u0bb0\u0bbf \u0b95\u0bbf\u0bb2\u0bcd\u0bb2\u0bbf\u0baf\u0bae\u0bcd", "\u0b9c\u0bbe\u0bb0\u0bcd\u0b9c\u0bcd \u0bb2\u0bc2\u0b95\u0bbe\u0bb8\u0bcd", "\u0b9c\u0bbe\u0b99\u0bcd \u0baf\u0bbe\u0bae\u0bc6", "\u0b9f\u0bc6\u0bb0\u0bcd\u0bb0\u0bc6\u0ba9\u0bcd\u0bb8\u0bcd \u0bae\u0bbe\u0bb2\u0bbf\u0b95\u0bcd", "\u0bae\u0bc0\u0bb2\u0bca\u0bb8\u0bcd \u0baa\u0bcb\u0bb0\u0bcd\u0bae\u0ba9\u0bcd", "\u0baa\u0bc6\u0bb8\u0bcd\u0b9f\u0bcb\u0ba9\u0bcd \u0b9a\u0ba4\u0bc1\u0bb0\u0bcd\u0b95\u0bcd\u0bb8\u0bcd", "\u0baa\u0bbf\u0bb0\u0bc6\u0b9f\u0bcd \u0b9a\u0bbf\u0ba9\u0bcd\u0ba9\u0bc7\u0bae\u0bcd\u0ba9\u0ba9\u0bcd", "\u0bae\u0bc8\u0b95\u0bcd \u0ba8\u0bbf\u0b95\u0bcd\u0b95\u0bcb\u0bb2\u0bb8\u0bcd", "\u0b9c\u0bbe\u0ba9\u0bcd \u0baa\u0bc2\u0bb0\u0bcd\u0bae\u0ba9\u0bcd", "\u0bb8\u0bcd\u0b9f\u0bc0\u0baa\u0ba9\u0bcd \u0baa\u0bbf\u0bb0\u0bc6\u0b9e\u0bcd\u0bb0\u0bcd\u0bb8\u0bcd", "\u0bb9\u0baf\u0bb5\u0bcd \u0bae\u0bbf\u0baf\u0b9a\u0bbe\u0b95\u0bbf", "\u0baa.\u0bb5\u0bcd. \u0bae\u0bc1\u0bb0\u0ba3\u0bc8\u0b95\u0bcd\u0b95\u0bc1", "\u0baa\u0bc6\u0b9f\u0bc6\u0bb0\u0bbf\u0b95\u0bcb \u0baa\u0bbf\u0bb3\u0bbf\u0ba9\u0bbf", "\u0bb8\u0bcd\u0b9f\u0bc0\u0bb5\u0ba9\u0bcd \u0b9a\u0bcb\u0b9f\u0bc6\u0bb0\u0bcd\u0baa\u0bc7\u0bb0\u0bcd\u0b95\u0bcd\u0bb9\u0bcd", "\u0baa\u0bbf\u0bb0\u0bbe\u0b99\u0bcd\u0b95\u0bcd \u0b95\u0bbe\u0baa\u0bcd\u0bb0\u0bbe", "\u0b95\u0bc6\u0ba9\u0bcd\u0ba9\u0ba4\u0bcd \u0baa\u0bcd\u0bb0\u0bbe\u0ba3\u0bbe\u0b9c\u0bbf\u0bb9\u0bcd", "\u0b95\u0bc7\u0bae\u0bb0\u0bc2\u0ba9\u0bcd \u0b95\u0bbf\u0bb0\u0bcc\u0baf\u0bc7", "\u0b86\u0bb2\u0bbf\u0bb5\u0bb0\u0bcd \u0bb8\u0bcd\u0b9f\u0bcb\u0ba9\u0bcd", "\u0bb0\u0bbe\u0baa\u0bb0\u0bcd\u0b9f\u0bcd \u0b85\u0bb2\u0bcd\u0b9f\u0bcd\u0bae\u0ba9\u0bcd", "\u0baa\u0bbf\u0bb0\u0bbf\u0b9f\u0bcd\u0bb8\u0bcd \u0bb2\u0bbe\u0b99\u0bcd", "\u0bb9\u0bcb\u0bb5\u0bb0\u0bcd\u0b9f\u0bcd \u0bb9\u0bbe\u0bb5\u0